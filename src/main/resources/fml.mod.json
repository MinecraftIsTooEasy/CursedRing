{
  "schemaVersion": 1,
  "id": "cursed-ring",
  "version": "${version}",
  "depends": {
    "fishmodloader": ">=3.2.3",
    "baubles": ">=1.0.6",
    "rusted_iron_core": ">=1.3.3",
    "many-lib": ">=2.2.0"
  },
  "environment": "*",
